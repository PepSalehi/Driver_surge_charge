   ],
   "source": [
    "x = cvx.Variable()\n",
    "y = cvx.Variable()\n",
    "constraints = [x + y <= 3]\n",
    "prob2 = cvx.Problem(cvx.Maximize(x + y), constraints = constraints)\n",
    "sol = prob2.solve(solver=cvx.MOSEK)\n",
    "print(\"optimal value\", sol)"
   ]
